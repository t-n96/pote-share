<h3>予約ルーム一覧</h3>

<table>
  <thead>
    <tr>
      <th>ルーム画像</th>
      <th>ルーム名</th>
      <th>合計料金</th>
      <th>開始日</th>
      <th>終了日</th>
    </tr>
  </thead>

  <tbody>
    <% @reservations.each do |reservation| %>
    <tr>
     
     
      <td><%= reservation.price %></td>
      <td><%= reservation.start_date %></td>
      <td><%= reservation.end_date %></td>
    </tr>
    <% end %>
  </table>

</table>